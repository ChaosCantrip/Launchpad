{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "palette": {
        "rosewater": "#F5e0dc",
        "flamingo": "#F2CDCD",
        "pink": "#F5C2E7",
        "mauve": "#CBA6D7",
        "red": "#F38BA8",
        "maroon": "#EBA0AC",
        "peach": "#FAB387",
        "yellow": "#F9E2AF",
        "green": "#A6E3A1",
        "teal": "#94E2D5",
        "sky": "#89DCEB",
        "sapphire": "#74C7EC",
        "blue": "#89B4FA",
        "lavender": "#B4BEFE",
        "text": "#CDD6FA",
        "base": "#1E1E2E"
    },
    "upgrade": {
      "source": "cdn",
      "interval": "168h",
      "auto": false,
      "notice": false
    },
    "blocks": [
      {
        "type": "prompt",
        "alignment": "left",
        "segments": [
            {
                "type": "docker",
                "style": "powerline",
                "leading_diamond": "\ue0b6",
                "powerline_symbol": "\ue0b0",
                "foreground": "#000000",
                "background": "#0B59E7",
                "template": "  {{ .Context }} "
            },
            {
                "template": " {{ .UserName }} ",
                "foreground": "p:base",
                "powerline_symbol": "\ue0b0",
                "background": "p:blue",
                "type": "session",
                "style": "powerline"
            },
            {
                "properties": {
                "folder_icon": "..\ue5fe..",
                "home_icon": "~",
                "style": "agnoster_short"
                },
                "template": " {{ .Path }} ",
                "foreground": "p:base",
                "powerline_symbol": "",
                "background": "p:pink",
                "type": "path",
                "style": "powerline"
            },
            {
                "properties": {
                "branch_icon": "\ue725 ",
                "cherry_pick_icon": "\ue29b ",
                "commit_icon": "\uf417 ",
                "fetch_status": false,
                "fetch_upstream_icon": false,
                "merge_icon": "\ue727 ",
                "no_commits_icon": "\uf0c3 ",
                "rebase_icon": "\ue728 ",
                "revert_icon": "\uf0e2 ",
                "tag_icon": "\uf412 "
                },
                "template": " {{ .HEAD }} ",
                "foreground": "p:base",
                "powerline_symbol": "\ue0b0",
                "background": "p:lavender",
                "type": "git",
                "style": "powerline"
            },
            {
                "template": " \ue0b1",
                "foreground": "p:red",
                "type": "text",
                "style": "plain"
            }
        ]
      }
    ],
    "version": 3,
    "final_space": true
  }
  